<?xml version='1.0' encoding='utf-8'?>
<widget id="es.guinotepro.guinotepro" version="2.0.0" versioncode="10023" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>GuiñotePro</name>
    <description>
        Videojuego de guiñote
    </description>
    <author email="ivan@lopez.es" href="http://www.guiñotepro.es">
        Iván López Asín
    </author>
    <content src="index.html" />
	<access origin="*"/>
<access origin="tel:*" launch-external="yes"/>
<allow-navigation href="*"/>
<allow-navigation href="http://*/*"/>
<allow-navigation href="https://*/*"/>
<allow-navigation href="data:*"/>
<allow-intent href="*"/>
<access origin="*"/>

 <preference name='phonegap-version' value='cli-7.0.1' />
 <preference name='pgb-builder-version' value='1' />
	


	<preference name="Orientation" value="landscape" /> 
	
	
	<preference name="KeepRunning" value="false"/>
 <preference name="exit-on-suspend" value="true" /> 

  <!-- <plugin name="cordova-plugin-background-mode" /> -->
  

	
	
<plugin name="cordova-plugin-powermanagement" />		
<plugin name="cordova-plugin-device" spec="1.1.7" />
<plugin name="cordova-plugin-googleplus" spec="5.1.1" />
<!-- <plugin name="cordova-plugin-inappbrowser" spec="1.7.0" /> -->
<plugin name="cordova-plugin-inappbrowser-wkwebview" />
<plugin name="cordova-plugin-wkwebview-engine" />
<plugin name="cordova-plugin-nativeaudio" spec="3.0.9" />
<plugin name="cordova-plugin-network-information" spec="1.3.2" />
<plugin name="cordova-plugin-screenorientation-landscape" spec="1.0.22" />
<plugin name="cordova-plugin-speechrecognition" spec="1.2.0" />
<plugin name="cordova-plugin-splashscreen" spec="4.0.2" />
<plugin name="cordova-plugin-vibration" spec="2.1.4" />
<plugin name="cordova-plugin-whitelist" spec="1.3.2" />
<plugin name="cordova-plugin-google-analytics" />
	
	

	<gap:plugin name="cordova-plugin-googleplus" source="npm" spec="5.1.1">
  <param name="REVERSED_CLIENT_ID" value="com.googleusercontent.apps.713259248814-9hbf4r1p4a9n2cu69mj8msd10a7r2err" />
</gap:plugin>
	
